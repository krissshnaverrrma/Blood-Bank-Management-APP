"builds": [
  {
    "src": "wsgi.py", 
    "use": "@vercel/python",
    "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
  }
],
"routes": [
  { "src": "/(.*)", "dest": "wsgi.py" }
]
{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb", "runtime": "python3.11" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app.py" 
    }
  ]
}